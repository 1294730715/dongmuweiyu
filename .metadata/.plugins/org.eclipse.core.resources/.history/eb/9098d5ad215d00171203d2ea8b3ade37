    <script type="text/javascript">
     function setTab(m,n){
  
		  for(var i=1;i<=n;i++){
    alert(i);
		   var clickLi= document.getElementById(i);
		   var con=document.getElementById("demo"+i);
		   if(i==m){
		    alert(i);
		   con.style.display="block";
		   clickLi.removeClss(active);
		   continue;
		   }else{
		   con.style.display="none";
		   clickLi.addClss(active);
		    continue;
		   }
		   // con.style.display=i==m?"block":"none";
		     } 
    } 

    
    </script>

